# API 서버가 실행될 포트를 지정합니다.
API_PORT=

# DB 관련 설정
# 데이터베이스 서버의 호스트 주소입니다.
DB_HOST=
# 데이터베이스 이름입니다.
DB_NAME=
# 데이터베이스 연결에 사용할 사용자 이름입니다.
DB_USERNAME=
# 데이터베이스 연결에 사용할 비밀번호입니다.
DB_PASSWORD=
# 데이터베이스 연결에 필요한 드라이버나 클래스 이름입니다.
DB_CLASSNAME=
# 데이터베이스 서버가 사용하는 포트 번호입니다.
DB_PORT=

# BASE_URL: 외부 API 혹은 애플리케이션의 기본 URL을 설정합니다.
BASE_URL=

# LLM_PROVIDER: 사용할 LLM 공급자를 설정합니다. (예: "ollama" 또는 "gemini")
LLM_PROVIDER=

# HUGGING_FACE_TOKEN: Hugging Face API 사용을 위한 인증 토큰을 설정합니다.
HUGGING_FACE_TOKEN=

# OLLAMA_MODEL: Ollama를 사용할 경우, 사용할 모델의 이름을 지정합니다.
OLLAMA_MODEL=

# GEMINI_API_KEY: Gemini API를 사용할 때 필요한 인증 키입니다.
GEMINI_API_KEY=
# GEMINI_API_URL: Gemini API의 엔드포인트 URL을 설정합니다.
GEMINI_API_URL=

# LLM_API_REQUESTS_PER_MINUTE: LLM API의 분당 요청 횟수 제한을 설정합니다.
LLM_API_REQUESTS_PER_MINUTE=

# SAMPLE_TAGS: 테스트용 임시 태그 데이터를 JSON 문자열 형식으로 설정합니다.
# 예: [{"code": "club", "label": "동아리 활동", "llm_desc": "설명"}]
SAMPLE_TAGS=

# SAMPLE_MESSAGES: 테스트용 임시 메시지 데이터를 JSON 문자열 형식으로 설정합니다.
# 예: [{"subject_id": "1621", "message": "메시지 내용"}]
SAMPLE_MESSAGES=